{
    "files.exclude": {
        ".cache": true
    },
    "files.associations": {
        "library.h": "c",
        ".clang-format": "yaml",
        "*.tcc": "c",
        "system_error": "c",
        "array": "c",
        "functional": "c",
        "regex": "c",
        "tuple": "c",
        "type_traits": "c",
        "utility": "c"
    },
    "editor.inlayHints.enabled": "off",
    "[c]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    }
}
